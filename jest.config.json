{
  "setupFiles": [
    "dotenv/config"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "watchPlugins": [
    "jest-watch-typeahead/filename",
    "jest-watch-typeahead/testname"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js",
    "\\.(png|gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
  },
  "modulePaths": [
    "/src/"
  ]
}