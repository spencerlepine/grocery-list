{
  "name": "quickcart",
  "version": "1.0.0",
  "author": "Spencer Lepine",
  "description": "Organize and budget a personal grocery cart for easy shopping",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlepine/quickcart"
  },
  "keywords": [
    "groceries",
    "firebase",
    "quickcart"
  ],
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "prepare": "husky install",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-staged": "eslint src --ext .js && eslint src --fix --ext .js && git add ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.1",
    "enzyme": "^3.11.0",
    "firebase": "^8.9.1",
    "material-ui-search-bar": "^1.0.0",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.1",
    "react-fda-nutrition-facts": "^0.1.0",
    "react-file-base64": "^1.0.3",
    "react-notifications-component": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-webcam-barcode-scanner": "^0.0.2-rc2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@upstatement/eslint-config": "^1.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && git add ."
    }
  }
}