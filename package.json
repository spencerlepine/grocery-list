{
  "name": "quickcart",
  "version": "1.0.0",
  "author": "Spencer Lepine",
  "description": "Organize and budget a personal grocery cart for easy shopping",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlepine/quickcart"
  },
  "keywords": [
    "groceries",
    "firebase",
    "quickcart"
  ],
  "engines": {
    "node": "v14.17.3",
    "npm": "7.20.3"
  },
  "scripts": {
    "dev": "webpack serve --config ./webpack.dev.config.js",
    "start": "node server/index.js",
    "build": "BABEL_ENV=node webpack --config ./webpack.prod.config.js",
    "create-hook": "husky add .husky/pre-commit 'npm run lint-staged && git add .'",
    "husky:prepare": "husky install && npm run create-hook",
    "test": "jest",
    "test:coverage": "npm run test -- --watchAll=false --coverage && coveralls < coverage/lcov.info",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch",
    "lint-staged": "eslint client/src --ext .js && eslint client/src --fix --ext .js && git add ."
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.1",
    "firebase": "^8.9.1",
    "material-ui-search-bar": "^1.0.0",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.1",
    "react-fda-nutrition-facts": "^0.1.0",
    "react-file-base64": "^1.0.3",
    "react-notifications-component": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "react-webcam-barcode-scanner": "^0.0.2-rc2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@upstatement/eslint-config": "^1.0.1",
    "@webpack-cli/generators": "^2.3.0",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^9.0.0",
    "coveralls": "^3.1.1",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.25.1",
    "express": "^4.17.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "https": "^1.0.0",
    "husky": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.4",
    "lodash": "^4.17.21",
    "style-loader": "^3.2.1",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && git add ."
    }
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "moduleDirectories": [
      "node_modules",
      "client/src"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/client/src/__mocks__/styleMock.js",
      "\\.(png|gif|ttf|eot|svg)$": "<rootDir>/client/src/__mocks__/fileMock.js"
    },
    "modulePaths": [
      "/client/src/"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "react-app",
      "eslint:recommended",
      "@upstatement/eslint-config/react",
      "plugin:react/recommended",
      "plugin:jest/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console": "off",
      "react-hooks/exhaustive-deps": "off",
      "react/display-name": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}